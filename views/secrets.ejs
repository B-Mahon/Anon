  <title>Posts</title>
</head>
<nav class="navbar navbar-dark bg-dark justify-content-between mb-5">
  <div class="container">
    <a class="navbar-brand">
      <i class="fas fa-mask"></i>
      Anon
    </a>
  <form class="form-inline">
      <a class="btn btn-outline-success" href="/submit">POST</a>
      <a class="btn btn-outline-secondary" href="/logout">Log Out</a>
    </form>
    </div>
</nav>
<body>
<div class="container" style="text-align: center;">
<h1>Secrets</h1>
<% userWithSecrets.forEach(function(user){%>
<div>
  <p class="secret-text"><%=user.secret%><i class="far fa-thumbs-up" name="<%=user.googleId%>"><%=user.likes%></i></p>
</div>
<% })%>
<script
  src="https://code.jquery.com/jquery-3.4.1.js"
  integrity="sha256-WpOohJOqMqqyKL9FccASB9O0KwACQJpFTUBLTYOVvVU="
  crossorigin="anonymous"></script>
<script src="javascript\client.js"></script>
</body>

